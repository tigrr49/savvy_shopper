<div class="page-header">
  <h3>Price Entry #<%= @price_entry.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Item</dt>
      <dd><%= @price_entry.item_id %></dd>

      <dt>Store</dt>
      <dd><%= @price_entry.store_id %></dd>

      <dt>Price</dt>
      <dd><%= @price_entry.price %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/price_entries" class="btn btn-primary">
        Back
      </a>
      <a href="/price_entries/<%= @price_entry.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_price_entry/<%= @price_entry.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
